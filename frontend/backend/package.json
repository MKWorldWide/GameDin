{
  "name": "gamedin-backend",
  "version": "5.0.0",
  "description": "GameDin Backend - Modernized with AWS SDK v3, Novasanctum AI, and Divina-L3 Integration",
  "main": "index.js",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.{ts,js}",
    "lint:fix": "eslint src/**/*.{ts,js} --fix",
    "format": "prettier --write src/**/*.{ts,js,json}",
    "deploy": "amplify push",
    "deploy:prod": "amplify push --yes",
    "logs": "amplify console",
    "clean": "rm -rf dist node_modules package-lock.json",
    "install:clean": "npm run clean && npm install"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.540.0",
    "@aws-sdk/lib-dynamodb": "^3.540.0",
    "@aws-sdk/client-cloudwatch": "^3.540.0",
    "@aws-sdk/client-cognito-identity": "^3.540.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.540.0",
    "@aws-sdk/client-s3": "^3.540.0",
    "@aws-sdk/client-lambda": "^3.540.0",
    "@aws-sdk/client-apigatewaymanagementapi": "^3.540.0",
    "@aws-amplify/backend": "^0.10.0",
    "@aws-amplify/backend-cli": "^0.10.0",
    "graphql": "^16.8.1",
    "apollo-server-lambda": "^3.13.0",
    "apollo-server-core": "^3.13.0",
    "graphql-tag": "^2.12.6",
    "graphql-tools": "^8.13.1",
    "joi": "^17.12.0",
    "zod": "^3.23.8",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1",
    "nanoid": "^5.0.6",
    "date-fns": "^3.6.0",
    "lodash": "^4.17.21",
    "ramda": "^0.29.1",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "helmet": "^8.0.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "rate-limiter-flexible": "^4.0.1",
    "express-rate-limit": "^7.3.1",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.33.2",
    "node-cron": "^3.0.3",
    "ioredis": "^5.3.2",
    "redis": "^4.6.13",
    "ws": "^8.18.0",
    "socket.io": "^4.8.1",
    "socket.io-redis": "^6.1.1",
    "bull": "^4.12.2",
    "bull-board": "^5.10.0",
    "nodemailer": "^6.9.12",
    "handlebars": "^4.7.8",
    "twilio": "^4.23.0",
    "stripe": "^14.21.0",
    "novasanctum-ai": "^1.0.0",
    "divina-l3-client": "^1.0.0",
    "ethers": "^6.11.1",
    "viem": "^2.7.9",
    "wagmi": "^2.5.7",
    "web3": "^4.4.0",
    "ipfs-http-client": "^60.0.1",
    "multiformats": "^13.0.1",
    "dag-pb": "^4.0.1",
    "dag-cbor": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.10",
    "@types/aws-lambda": "^8.10.136",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/uuid": "^9.0.8",
    "@types/lodash": "^4.14.202",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/ws": "^8.5.10",
    "typescript": "^5.5.3",
    "tsx": "^4.19.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.12",
    "ts-jest": "^29.1.2",
    "supertest": "^7.0.0",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^23.0.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^11.0.2"
  },
  "keywords": [
    "gamedin",
    "backend",
    "aws",
    "amplify",
    "graphql",
    "lambda",
    "dynamodb",
    "novasanctum",
    "divina-l3",
    "blockchain",
    "ai",
    "gaming"
  ],
  "author": "GameDin Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gamedin/backend.git"
  },
  "bugs": {
    "url": "https://github.com/gamedin/backend/issues"
  },
  "homepage": "https://gamedin.com",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
} 